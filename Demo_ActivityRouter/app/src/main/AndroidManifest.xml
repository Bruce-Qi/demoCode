<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="wwtao.demo.demo_activityrouter">

    <application
            android:name=".App"
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <activity android:name=".activities.ActivityA" android:label="ActivityA"/>
        <activity android:name=".activities.ActivityB" android:label="ActivityB"/>
        <activity android:name=".activities.ActivityC" android:label="ActivityC"/>
        <activity android:name=".activities.ActivityD" android:label="ActivityD"/>
        <activity android:name=".activities.ActivityE" android:label="ActivityE">
            <intent-filter>
                <action android:name="android.intent.action.VIEW"/>
                <category android:name="android.intent.category.DEFAULT"/>
                <data android:host="implicit1" android:scheme="wwtao"/>
            </intent-filter>

        </activity>
        <activity android:name=".activities.ActivityF" android:label="ActivityF">
            <intent-filter>
                <action android:name="android.intent.action.VIEW"/>
                <category android:name="android.intent.category.DEFAULT"/>
                <data android:host="implicit2" android:scheme="wwtao"/>
            </intent-filter>
        </activity>
    </application>

</manifest>